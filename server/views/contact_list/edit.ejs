<%- include ../../views/partials/header.ejs%>
<!-- Main nav -->
<%- include ../../views/partials/main_nav.ejs%>



<main class="container">
<div class="p-5 mb-4 bg-light rounded-3">
    <div class="container-fluid py-5">
      <h1 class="display-5 fw-bold">Welcome to <%= title %> Page</h1>
     
    <br>
    <div class="">

   <h1> <%= title %></h1>

   <form  class="form" method="post">
        <div class="form-group">
            <label for="">Name</label>
            <input type="text" class="form-control" id=
            "nameTextField" placeholder="Enter contact name" name="name" value="<%= list.name %>" required>
        </div>


        <div class="form-group">
            <label for="">Number</label>
            <input type="text" class="form-control" id=
            "numberTextField" placeholder="Enter contact name" name="number"  value="<%= list.number %>" required>
        </div>

        <div class="form-group">
            <label for="">Email</label>
            <input type="text" class="form-control" id=
            "emailTextField" placeholder="Enter contact name" name="email" value="<%= list.email %>" required>
        </div>
        <br>
        <button class="btn btn-primary" type="submit">
            <i class="fas fa-edit"></i>Submit
        </button>
       

        <a href="/Contact-list" class="btn btn-warning"><i class="fas fa-undo"></i>Cancel</a>
   </form>

    </div>
       
    </div>

    
    </div>

    
  <!-- Bottom Nav-->
  <%- include ../../views/partials/bottom_nav.ejs%>

  <%- include ../../views/partials/footer.ejs%>